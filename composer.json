{
    "name": "wappcode/pdss-utilities",
    "description": "Utilidades para PHP Doctrine Server Side",
    "type": "library",
    "scripts": {
        "updatedb": "doctrine orm:schema-tool:update --force",
        "test": [
            "phpunit"
        ]
    },
    "require": {
        "php": ">7.1",
        "doctrine/orm": ">=2.0"
    },
    "autoload": {
        "psr-4": {
            "PDSSUtilities\\": "PDSSUtilities/src/"
        }
    },
    "require-dev": {
        "doctrine/dbal": "3.1.4",
        "doctrine/annotations": "^1.13",
        "symfony/yaml": "2.*",
        "symfony/cache": "^5.3",
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "AppLibrary\\": "dev/library/",
            "AppEntities\\": "dev/entities/",
            "AppTest\\": "dev/tests/"
        }
    },
    "license": "MIT"
}